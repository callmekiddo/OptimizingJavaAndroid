<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp">

    <Button
        android:id="@+id/btnStringPlus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Insert using String Plus" />
    <TextView
        android:id="@+id/tvStringPlusTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Time: "
        android:paddingTop="8dp" />

    <Button
        android:id="@+id/btnStringFormat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Insert using String.format" />
    <TextView
        android:id="@+id/tvStringFormatTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Time: "
        android:paddingTop="8dp" />

    <Button
        android:id="@+id/btnStringBuilder"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Insert using StringBuilder" />
    <TextView
        android:id="@+id/tvStringBuilderTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Time: "
        android:paddingTop="8dp" />

    <Button
        android:id="@+id/btnCompileStatement"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Insert using Compiled Statement" />
    <TextView
        android:id="@+id/tvCompileStatementTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Time: "
        android:paddingTop="8dp" />

    <Button
        android:id="@+id/btnContentValues"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Insert using ContentValues" />
    <TextView
        android:id="@+id/tvContentValuesTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Time: "
        android:paddingTop="8dp" />

    <Button
        android:id="@+id/btnCompileStatementOneTransaction"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Insert using Compiled Statement One Transaction" />
    <TextView
        android:id="@+id/tvCompileStatementOneTransactionTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Time: "
        android:paddingTop="8dp" />

    <Button
        android:id="@+id/btnIterateBothColumns"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Iterate Both Columns" />
    <TextView
        android:id="@+id/tvIterateBothColumnsTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Time: "
        android:paddingTop="8dp" />

    <Button
        android:id="@+id/btnIterateFirstColumn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Iterate First Column" />
    <TextView
        android:id="@+id/tvIterateFirstColumnTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Time: "
        android:paddingTop="8dp" />

</LinearLayout>
